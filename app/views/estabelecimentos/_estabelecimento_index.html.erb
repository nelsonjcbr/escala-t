<div id="<%= dom_id estabelecimento %>">

  <div class="card">
    <div class="card-header">
      <h4 class="card-title">        
        <%= link_to estabelecimento.nome, estabelecimento_path(estabelecimento) %>
      </h4>
    </div>
    <div class="card-body"> 
      <div class="row">
        <div class="col-md-3">
          <label>CNPJ:</label>
        </div>
        <div class="col-md-9">
          <strong><%= estabelecimento.cnpj %></strong>
        </div>
      </div>
     <div class="row">
        <div class="col-md-3">
          <label>CNES:</label>
        </div>
        <div class="col-md-9">
          <strong><%= estabelecimento.cnes %></strong>
        </div>
      </div>
      <div class="row">
        <div class="col-md-3">
          <label>Cidade:</label>
        </div>
        <div class="col-md-9">
            <strong><%= estabelecimento.cidade_uf %></strong>
        </div>
      </div>
      <div class="row">
        <div class="col-md-3">
          <label><%= link_to "Unidades (#{estabelecimento.unidades.size})", estabelecimento_unidades_url(estabelecimento) %></label>
        </div>
      </div>
    </div>
  </div>
</div>

