<%= turbo_frame_tag equipe do %>

  <div class="card">
    <div class="card-body">
      <div class="row match-height">
        <div class="col-md-4 col-12"><b><%= link_to equipe.nome, equipe_path(equipe) %></b></div>
        <div class="col-md-4 col-12"><%= equipe.unidade.nome %></div>
      </div>
    </div>
  </div>

<% end %>